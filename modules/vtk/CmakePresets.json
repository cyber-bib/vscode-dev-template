{
  "version": 3,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 20,
    "patch": 0
  },
  "configurePresets": [
    {
      "name": "win-emsdk-vtk",
      "displayName": "win-emsdk-vtk",
      "description": "Configures VTK with Emscripten on Windows",
      "inherits": "win-emsdk",
      "cacheVariables": {
        "BUILD_SHARED_LIBS": {
          "type": "BOOL",
          "value": "OFF"
        },
        "CMAKE_BUILD_TYPE": {
          "type": "STRING",
          "value": "Release"
        },
        "VTK_ENABLE_LOGGING": {
          "type": "BOOL",
          "value": "OFF"
        },
        "VTK_ENABLE_WRAPPING": {
          "type": "BOOL",
          "value": "OFF"
        },
        "VTK_GROUP_ENABLE_Imaging": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_GROUP_ENABLE_MPI": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_GROUP_ENABLE_Qt": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_GROUP_ENABLE_Rendering": {
          "type": "STRING",
          "value": "WANT"
        },
        "VTK_GROUP_ENABLE_StandAlone": {
          "type": "STRING",
          "value": "WANT"
        },
        "VTK_GROUP_ENABLE_Views": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_GROUP_ENABLE_Web": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_LEGACY_REMOVE": {
          "type": "BOOL",
          "value": "ON"
        },
        "VTK_MODULE_ENABLE_VTK_hdf5": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_MODULE_ENABLE_VTK_sqlite": {
          "type": "STRING",
          "value": "NO"
        },
        "VTK_MODULE_ENABLE_VTK_InteractionStyle": {
          "type": "STRING",
          "value": "WANT"
        },
        "VTK_MODULE_ENABLE_VTK_InteractionWidgets": {
          "type": "STRING",
          "value": "WANT"
        },
        "VTK_MODULE_ENABLE_VTK_RenderingContext2D": {
          "type": "STRING",
          "value": "DONT_WANT"
        },
        "VTK_MODULE_ENABLE_VTK_RenderingContextOpenGL2": {
          "type": "STRING",
          "value": "DONT_WANT"
        },
        "VTK_USE_SDL2": {
          "type": "BOOL",
          "value": "ON"
        },
        "VTK_OPENGL_USE_GLES": {
          "type": "BOOL",
          "value": "ON"
        }
      },
      "binaryDir": "./build/windows/emsdk/modules/vtk"
    }
  ],
  "buildPresets": [
  ]
}